<table class="table table-responsive-md table-dark table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Address</th>
                <th>Additional Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zipcode</th>
                <th>Contact</th>
                <th>Phone</th>
                <th>Fax</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            {{#each carriers}}
            <tr>
                <td>{{carrier_name}}</td>
                <td>{{carrier_address}}</td>
                <td>{{additional_address}}</td>
                <td>{{city}}</td>
                <td>{{state_code}}</td>
                <td>{{zip}}</td>
                <td>{{contact}}</td>
                <td>{{phone}}</td>
                <td>{{fax}}</td>
                <td>{{email}}</td>
            </tr>
            {{/each}}
        </tbody>
