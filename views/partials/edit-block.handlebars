<div class="modal fade" id="staticEdit" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">
                    Edit Carrier Details
                </h5>
                {{!-- <h6>{{}}</h6> --}}
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">

                <select class="form-select" aria-label="Default select example">
                    <option selected>Select an Attribute to edit</option>
                    <option value="carrier_name">Name</option>
                    <option value="carrier_address">Address</option>
                    <option value="address_2">Additional Address</option>
                    <option value="city">City</option>
                    <option value="state_code">State</option>
                    <option value="zip">Zip</option>
                    <option value="contact">Contact</option>
                    <option value="phone">Phone</option>
                    <option value="fax">Fax</option>
                    <option value="email">Email</option>
                </select>
                <br>
                {{!-- input element to only appear once user makes a selection above --}}
                <input type="text" class="form-control hidden value-input" placeholder="New Value"
                    aria-label="New Value" aria-describedby="basic-addon8" required="true" id="new-value-edit">


            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancel</button>
                <button type="button" class="submit-update btn btn-primary" data-bs-dismiss="modal">Add</button>
            </div>
        </div>
    </div>
</div>